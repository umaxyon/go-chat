scalar Time

type Message {
  id: String!
  user: String!
  createdAt: Time!
  text: String!
}

type User {
  user: String!
}

type Mutation {
  postMessage(user: String!, text: String!): Message
}

type Query {
  messages: [Message!]!
  users: [User!]!
}

type Subscription {
  subscribeMessage(user: String!): Message
}